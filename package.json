{
  "name": "troposheric",
  "version": "0.0.1",
  "description": "usertoken data connector",
  "keywords": [
    "firebase",
    "memory02",
    "primus",
    "token-memory"
  ],
  "license": "MIT",
  "author": "Alex Davis <alex@alex2006hw.com>",
  "main": "cluster.js",
  "repository": {},
  "scripts": {
    "build": "npm run format;npm run build:server",
    "postbuild": "cp ./node_modules/gun/gun.* server/;cp ./node_modules/gun/gun.* public/js/",
    "build:server": "./node_modules/babel-cli/bin/babel.js src -d ./",
    "clean": "npm run clean:root;npm run clean:tmp &",
    "clean:cache": "mv ~/.cache/troposheric ~/.cache/troposheric-deleted;rm -rf ~/.cache/troposheric-deleted;npm run clean:tmp &",
    "clean:root": "rm -rf node_modules configs server tests wip01;mkdir -p ~/.cache/troposheric/root/node_modules;ln -s ~/.cache/troposheric/root/node_modules ",
    "clean:tmp": " rm -rf /tmp/fbfn* /tmp/firebase_functions* /tmp/npm* &",
    "format": "npm-run-all -l -p format:src format:package",
    "format:package": "./node_modules/sort-package-json/index.js &",
    "format:src": "./node_modules/prettier/bin/prettier.js --write \"src/**/*{.js, .json}\" &",
    "start": "npm-run-all -l -p start:server",
    "start:client": "cd public",
    "start:server": "node server/cluster.js",
    "test": "cd tests;node client.js"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.2",
    "cluster": "^0.7.7",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "gun": "^0.9.4",
    "jwt-simple": "^0.5.1",
    "level-hyper": "^1.2.2",
    "leveldown": "^2.1.1",
    "levelup": "^2.0.1",
    "object-assign": "^4.1.1",
    "primus": "^7.1.0",
    "serve-favicon": "^2.4.5",
    "ws": "^3.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.9.2",
    "sort-package-json": "^1.7.1"
  },
  "engines": {
    "node": "6.x"
  }
}
